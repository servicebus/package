  "bugs": {
    "url": "https://github.com/servicebus/package/issues"
  },
  "homepage": "https://github.com/servicebus/package#readme",
  "devDependencies": {
    "husky": "^1.3.1",
    "jest": "^24.7.0",
    "lint-staged": "^8.1.5",
    "prettier-standard": "^9.1.1"
  }
}

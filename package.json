{
  "name": "@servicebus/package",
  "version": "0.0.0-development",
  "description": "middleware to package messages into uniform format",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "format": "prettier-standard 'index.js'"
  },
